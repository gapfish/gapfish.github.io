{
  "$schema" : "http://json-schema.org/draft-04/schema#" ,
  "type" : "object" ,
  "properties" : {
    "installations": {
      "type": "object" ,
      "properties": {
        "id" : {
          "type" : "string"
        } ,
        "deviceType" : {
          "type" : "string" ,
          "enum": [ "ios", "android" ]
        } ,
        "installationId" : {
          "type" : "string"
        } ,
        "deviceToken" : {
          "type" : "string"
        }
      } ,
      "required": [ "id", "deviceType" ]
    }
  } ,
  "required" : [ "installations" ]
}
