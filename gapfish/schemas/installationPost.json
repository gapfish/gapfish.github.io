{
  "$schema" : "http://json-schema.org/draft-04/schema#" ,
  "type" : "object" ,
  "properties" : {
    "installations" : {
      "type": "object" ,
      "properties": {
        "deviceType" : {
          "type" : "string" ,
          "enum": [ "ios", "android" ]
        } ,
        "androidRegistrationId" : {
          "type" : "string"
        } ,
        "iosDeviceToken" : {
          "type" : "string"
        },
        "mobileCarrier" : {
          "type" : "string"
        }
      } ,
      "required": [ "deviceType" ]
    }
  } ,
  "required": [ "installations" ]
}
