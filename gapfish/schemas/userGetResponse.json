{
  "$schema": "http://json-schema.org/draft-04/schema#" ,
  "type": "object" ,
  "properties": {
    "user": {
      "type": "object" ,
      "properties": {
        "id": {
          "type": "string"
        } ,
        "first_name": {
          "type": "string"
        } ,
        "last_name": {
          "type": "string"
        } ,
        "locale": {
          "type": "string"
        } ,
        "currency": {
          "type": "string" ,
          "enum": [ "EUR", "CHF" ]
        } ,
        "stats": {
          "type": "object" ,
          "properties": {
            "balanceCents": {
              "type": "integer"
            }
          }
        }
      } ,
      "required": [ "id" ]
    }
  } ,
  "required": [ "user" ]
}
